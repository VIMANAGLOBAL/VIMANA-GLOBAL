{
    "name": "vimana-dashboard",
    "version": "0.0.1",
    "scripts": {
        "apidoc": "apidoc -i ./src/backend/routes/ -o ./docs/",
        "build:ts": "npm run copy && tsc",
        "build:webpack": "webpack --watch",
        "clean": "rimraf ./build",
        "copy": "cd ./src/email/dist && copyfiles ./* ./**/* ./**/**/* ../../../build/email && cd ../../backend && copyfiles ./views/*.ejs ../../build/backend && copyfiles ./util/validator/hostname/* ../../build/backend && copyfiles ./templates/* ../../build/backend && cd ./views && copyfiles ./favicon.png ../../../build/frontend/media",
        "dc:install": "docker-compose run modules install",
        "dc:test": "docker-compose run test",
        "dc:uninstall": "docker-compose down -v",
        "lint": "tslint --fix ./src/backend/**/*.ts",
        "start": "docker-compose up web",
        "start-local": "docker-compose up mailcatcher web",
        "start:app": "npm run build:ts && concurrently --kill-others \"npm run build:ts -- -w\" \"npm run build:webpack\" \"npm run server\"",
        "server": "nodemon --watch ./build/backend ./build/backend/app.js",
        "test": "jest"
    },
    "author": "michael lozinsky, Sergey Romanovsky",
    "license": "ISC",
    "jest": {
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "testRegex": "(/test/.*(test|spec))\\.(ts)$",
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "node"
        ]
    },
    "apidoc": {
        "name": "vimana-dashboard",
        "version": "0.0.1",
        "apidoc": {
            "title": "Viamana-Dashboard API documentation"
        }
    },
